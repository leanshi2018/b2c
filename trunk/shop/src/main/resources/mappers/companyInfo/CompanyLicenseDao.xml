        audit_code ,audit_remark
    </sql>

    <sql id="base_value_column">
        #{id},#{mCode},#{companyName},#{creditCode},#{companyAdd},#{companyPhone},#{bankName},#{bankId},#{businessLicense},#{permissionLicense},#{status},#{createTime},
